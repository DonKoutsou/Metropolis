{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Project",
            "justMyCode": false,
            "requireExactSource": false,
            "type": "coreclr",
            "request": "launch",
            
            "program": "E:/Godot/Godot_v3.5.3-stable_mono_win64/Godot_v3.5.3-stable_mono_win64.exe",
            // Change the arguments below for the project you want to test with.
            // To run the project instead of editing it, remove the "--editor" argument.
            "args": [
                "--path",
                "${workspaceRoot}"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "preLaunchTask": "build",
            "enableStepFiltering": true,
            "logToFile": true,
            "omnisharp.path": "latest"
            
          },
          {
            "name": "Run",
            "justMyCode": false,
            "requireExactSource": false,
            "enableStepFiltering": true,
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "E:/Godot/Godot_v3.5.3-stable_mono_win64/Godot_v3.5.3-stable_mono_win64.exe",
            "args": [
                "--path",
                "${workspaceRoot}"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart"
        },
        {
            "name": "Attach",
            "type": "mono",
            "request": "attach",
            "address": "localhost",
            "port": 23685,
            "internalConsoleOptions": "neverOpen",
        }
    ],
    "compounds": [
        {
            "name": "Run and attach",
            "configurations": [
                "Launch Project",
                "Attach",
            ]
        }
    ]
}